{
  "expo": {
    
    "name": "Pro Selektor",
    "slug": "Pro Selektor",
    "scheme": "Pro Selektor",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "orientation": "landscape",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBonjourServices": [
          "_ssh._tcp",
          "_http._tcp"
        ],
        "NSLocalNetworkUsageDescription": "This app needs access to the local network to detect devices.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Your app needs microphone access for audio streaming."
      },
      "bundleIdentifier": "io.cringemaster4k.gi-lyd-selektor"
    },
    "android": {
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_MULTICAST_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cringemaster4k.gi_lyd_selektor"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cb3643cd-8ada-416f-be58-664579850306"
      }
    },
    "owner": "cringemaster4k"
  }
}
